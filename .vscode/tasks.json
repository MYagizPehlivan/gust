{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo build"
		},
		{
			"type": "shell",
			"command": "gnome-terminal",
			"args": [
				"--",
				"${workspaceFolder}/target/debug/gust"
			],
			"label": "run",
			"dependsOn": [
				"rust: cargo build"
			],
			"problemMatcher": []
		}
	]
}